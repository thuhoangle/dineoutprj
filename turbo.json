{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
  // "globalEnv": [
  //   "NEXT_PUBLIC_SUPABASE_URL",
  //   "NEXT_PUBLIC_SUPABASE_ANON_KEY",
  //   "NEXT_PUBLIC_MAPLIBRE_API_KEY"
  // ]
}
